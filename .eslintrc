{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/prop-types": 0,
    "react/no-array-index-key": 0,
    "no-underscore-dangle": 0,
    "import/imports-first": ["error", "absolute-first"],
    "import/newline-after-import": "error",
    "import/prefer-default-export": 0,
    "no-use-before-define": 0,
    "no-param-reassign": 0,
    "func-names": 0,
    "no-console": 0,
    "no-shadow": 0,
    "no-nested-ternary": 0
  },
  "globals": {
    "window": true,
    "document": true,
    "localStorage": true,
    "FormData": true,
    "FileReader": true,
    "Blob": true,
    "navigator": true,
    "it": true,
    "describe": true,
    "expect": true,
    "fetch": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
